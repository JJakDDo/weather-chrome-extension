{
    "name": "Weather Chrome Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "geolocation",
        "https://kit.fontawesome.com/e41c456ab1.js"
    ],
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com; object-src 'self'"
  }